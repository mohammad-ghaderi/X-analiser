{
	"name": "x-analyser",
	"private": true,
	"version": "0.0.0",
	"type": "commonjs",
	"main": "electron/main.js",
	"scripts": {
		"start": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && npx electron .\"",
		"dev": "vite",
		"electron": "electron .",
		"build": "vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"electron:build": "electron-builder",
		"dist": "electron-builder"
	},
	"dependencies": {
		"@fortawesome/fontawesome-free": "^6.7.2",
		"axios": "^1.8.1",
		"exceljs": "^4.4.0",
		"express": "^4.21.2",
		"knex": "^3.1.0",
		"pdfkit": "^0.16.0",
		"react": "^19.0.0",
		"react-bootstrap": "^2.10.9",
		"react-dom": "^19.0.0",
		"react-router-dom": "^7.2.0",
		"sqlite3": "^5.1.7",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@eslint/js": "^9.19.0",
		"@types/react": "^19.0.8",
		"@types/react-dom": "^19.0.3",
		"@vitejs/plugin-react": "^4.3.4",
		"concurrently": "^9.1.2",
		"electron": "^34.2.0",
		"electron-builder": "^25.1.8",
		"eslint": "^9.19.0",
		"eslint-plugin-react": "^7.37.4",
		"eslint-plugin-react-hooks": "^5.0.0",
		"eslint-plugin-react-refresh": "^0.4.18",
		"globals": "^15.14.0",
		"vite": "^6.1.0",
		"wait-on": "^8.0.2"
	},
	"build": {
		"appId": "com.x-analyser",
		"productName": "X-analyser",
		"files": [
			"electron/**/*",
			"dist/**/*",
			"package.json",
			"package-lock.json",
			"src/assets/**/*"
		],
		"directories": {
			"output": "release"
		},
		"win": {
			"target": "nsis"
		}
	}
}
